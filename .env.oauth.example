# OAuth Authentication Configuration
# Use this if you're having issues with JWT authentication

SF_AUTH_METHOD=oauth
SF_SANDBOX=true

# Connected App credentials
SF_CLIENT_ID=your_connected_app_consumer_key
SF_CLIENT_SECRET=your_connected_app_consumer_secret

# User credentials
SF_USERNAME=your_salesforce_username@example.com
SF_PASSWORD=your_salesforce_password
SF_SECURITY_TOKEN=your_security_token

# Optional: Batch configuration
SF_MAX_CONCURRENT=3
SF_RATE_LIMIT_DELAY=1000
SF_MAX_RETRIES=3
SF_LOG_LEVEL=info